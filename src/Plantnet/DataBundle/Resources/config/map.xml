    <services>

        <service id="plantnet_bota_bundle.map.location" class="Plantnet\BotaBundle\Map\LocationMap">
            <tag name="vichgeo.map" alias="location" />
        </service>

        <service id="plantnet_bota_bundle.map.detail" class="Plantnet\BotaBundle\Map\DetailMap">
