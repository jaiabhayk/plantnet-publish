{% block main %}{% endblock %}
{% block javascripts %}
    <script type="text/javascript">
        $('.pagerfanta').on('click','a', function(event){
            event.preventDefault();
            if($(this).attr('href').length){
                var $url = $(this).attr('href');
                ajaxLoader();
                $('#data-load').load($url,function(){
                    $('#page-loading').empty();
                });
            }
        });
    </script>
{% endblock %}