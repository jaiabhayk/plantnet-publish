<ul class="nav nav-tabs nav-stacked">
	<li>
		<a href="{{ path('_module', { 'project': project, 'collection': collection.name, 'module': module.name }) }}">
			<i class="icon-list"></i>
			{{ module.name }}
			<small class="muted">{{ module.nbrows }} {{ "line-entities"|trans }}</small>
		</a>
	</li>
	{% if module.children|length > 0 %}
		{% for child in module.children if (child.type == 'image' and child.deleting != true) %}
			<li class="child">
				<a href="{{ path('_submodule', { 'project': project, 'collection': collection.name, 'module': module.name, 'submodule': child.name }) }}">
					<i class="icon-camera"></i>
					{{ child.name }}
					<small class="muted">{{ child.nbrows }} {{ "line-entities"|trans }}</small>
				</a>
			</li>
		{% endfor %}
		{% for child in module.children if (child.type == 'locality' and child.deleting != true) %}
			<li class="child">
				<a href="{{ path('_submodule', { 'project': project, 'collection': collection.name, 'module': module.name, 'submodule': child.name }) }}">
					<i class="icon-map-marker"></i>
					{{ child.name }}
					<small class="muted">{{ child.nbrows }} {{ "line-entities"|trans }}</small>
				</a>
			</li>
		{% endfor %}
	{% endif %}
	<li>
		<a href="{{ path('_module_search', { 'project': project, 'collection': collection.name, 'module': module.name }) }}">
			<i class="icon-search zz"></i>
			{{ "line-search-in"|trans|capitalize }} {{ module.name }}
		</a>
	</li>
</ul>